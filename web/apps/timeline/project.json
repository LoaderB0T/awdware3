{
  "projectType": "application",
  "sourceRoot": "apps/timeline/src",
  "prefix": "awd",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/timeline",
        "preserveSymlinks": true,
        "index": "apps/timeline/src/index.html",
        "main": "apps/timeline/src/main.ts",
        "polyfills": "apps/timeline/src/polyfills.ts",
        "tsConfig": "apps/timeline/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/timeline/src/favicon.ico", "apps/timeline/src/assets"],
        "styles": ["apps/timeline/src/styles.scss"],
        "scripts": [],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/styles/src/lib"]
        },
        "customWebpackConfig": {
          "path": "apps/timeline/webpack.config.js"
        },
        "commonChunk": false
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/timeline/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "timeline:build:production"
        },
        "development": {
          "browserTarget": "timeline:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4203,
        "publicHost": "http://localhost:4203"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/timeline/**/*.ts", "apps/timeline/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/timeline"],
      "options": {
        "jestConfig": "apps/timeline/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
