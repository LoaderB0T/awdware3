{
  "name": "awdware",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Janik Schumacher (LoaderB0T)",
    "url": "https://awdware.de"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "docker-build": "docker build -t awdware .",
    "docker-build-dev": "docker build -t awdware . -f dev.Dockerfile",
    "docker-build-dev-prepare": "nx construct -m build --build",
    "local-docker": "yarn docker-build-dev-prepare && yarn docker-build-dev && yarn docker-run",
    "docker-run": "docker run -it --rm -p 9000:80 awdware",
    "construct": "yarn nx construct -m jasc",
    "construct-mobile": "yarn nx construct -m host --host --watch"
  },
  "private": true,
